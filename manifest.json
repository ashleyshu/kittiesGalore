{
    "name": "attention tracker",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Timer"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs"],

    "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" }
}